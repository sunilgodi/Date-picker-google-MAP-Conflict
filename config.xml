<?xml version='1.0' encoding='utf-8'?>
<widget id="com.test.map" version="1.0.7" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Map demo</name>
    <description>
      map conflicting with date picker
    </description>
    <author email="dev@cordova.apache.org" href="">
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="Orientation" value="portrait" />
    <preference name="StatusBarBackgroundColor" value="#D84315" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashScreenBackgroundColor" value="red" />
    <preference name="deployment-target" value="9.0" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="target-device" value="handset" />
    <preference name="BackupWebStorage" value="local" />
    <preference name="cordova.plugins.diagnostic.modules" value="LOCATION" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-compat" spec="^1.1.0" />
    <plugin name="cordova-plugin-device" spec="~1.1.6" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.3" />
    <plugin name="cordova-plugin-file" spec="~4.3.3" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.6.3" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.3" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="~2.2.3" />
    <plugin name="cordova-plugin-appinfo" spec="^2.1.2" />
    <plugin name="cordova-plugin-datepicker" spec="~0.9.3" />
    <plugin name="ionic-plugin-keyboard" spec="~2.2.1" />
    <plugin name="phonegap-plugin-mobile-accessibility" spec="~1.0.5-dev" />
    <plugin name="cordova-plugin-contacts" spec="~2.3.1">
        <variable name="CONTACTS_USAGE_DESCRIPTION" value="To choose a number from contacts" />
    </plugin>
    <plugin name="cordova-plugin-x-socialsharing" spec="~5.1.8" />
    <plugin name="cordova-plugin-photo-library" spec="~2.0.4">
        <variable name="PHOTO_LIBRARY_USAGE_DESCRIPTION" value="To use Library to set profile picture and save reciepts" />
    </plugin>
    <plugin name="cordova-plugin-camera" spec="~2.4.1">
        <variable name="CAMERA_USAGE_DESCRIPTION" value="To use camera to set profile picture" />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="To use Library to set profile picture and save reciepts" />
    </plugin>
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.1" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <plugin name="cordova.plugins.diagnostic" spec="^3.9.2" />
    <plugin name="cordova-plugin-add-swift-support" spec="^1.7.2" />
    <plugin name="cordova-plugin-qrscanner" spec="~2.6.0" />
    <plugin name="cordova-plugin-googlemaps" spec="~2.5.0">
        <variable name="API_KEY_FOR_IOS" value="AIzaSyA4378zdrK_7RMCjEnEHga6YotxpOMkE4Q" />
    </plugin>
    <engine name="ios" spec="^4.5.5" />
</widget>
